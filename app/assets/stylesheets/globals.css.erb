.center-xy
{
	display: table;
	text-align: center;
}

.center-xy .centered
{
	display: table-cell;
	vertical-align: middle;
}

/* Inicio header */
.sky-header
{
	width: 100%;
	background-color: #FFF;
	position: relative;
	z-index: 3;
}

.sky-header,
.sky-header .link-evd,
.sky-header .link-ensenanza,
.sky-header .link-instruccion
{
	height: 70px;
}

.sky-header .link-evd
{
	position: absolute;
	top: 0px;
	left: 50%;
	width: 200px;
	margin-left: -100px;
	text-align: center;
	font-size: 45px;
	font-weight: bold;
	z-index: 6;
}

.sky-header .link-evd a
{
	color: #FFF;
	text-decoration: none;
}

.sky-header .link-evd:after,
.sky-header .link-evd:before
{
	content: ' ';
	position: absolute;
	top: 0px;
	background-repeat: no-repeat;
	background-size: auto 100%;
	height: 100%;
	width: 25px;
	z-index: 7;
}

.sky-header .link-evd:after
{
	left: -2px;
	/*background-image: url('<%= asset_data_uri 'tpl/borde1-left.png' %>');*/
	background-image: url('<%= asset_data_uri 'tpl/borde1-left.png' %>');
	background-position: top left;
}
.sky-header .link-evd:before
{
	right: -2px;
	background-image: url('<%= asset_data_uri 'tpl/borde1-right.png' %>');
	background-position: top right;
}

.sky-header .link-instruccion,
.sky-header .link-ensenanza
{
	position: absolute;
	top: 0px;
	text-align: center;
	font-size: 22px;
	z-index: 4;
}
.sky-header .link-instruccion a,
.sky-header .link-ensenanza a
{
	color: #000;
	text-decoration: none;
}

.sky-header .link-instruccion
{
	margin-right: 100px;
	padding-right: 5%;
	right: 50%;
}

.sky-header .link-ensenanza
{
	margin-left: 100px;
	padding-left: 5%;
	left: 50%;
}
/* Fin header */

/* FONDO HOME */
.sky-fondo-home
{
	width: 100%;
	height: 500px;
}
/* fin fondo Home */

.sky-lecciones
{
	padding-top: 30px;
	background-color: #005580;
	background-image: url('<%= asset_data_uri 'tpl/fondo-novedades.jpg' %>');
	background-size: 100% auto;
	background-repeat: repeat-y;
	position: relative;
	z-index: 4;
}

.sky-lecciones:before,
.sky-lecciones:after
{
	content: ' ';
	position: absolute;
	z-index: 5;
	width: 180px;
	height: 180px;
	background-size: 100% 100%;
}

.sky-lecciones:before
{
	top: 10px;
	left: 10px;
	background-image: url('<%= asset_data_uri 'tpl/esquina-top-left.png' %>');
}

.sky-lecciones:after
{
	top: 10px;
	right: 10px;
	background-image: url('<%= asset_data_uri 'tpl/esquina-top-right.png' %>');
}

.sky-lecciones .menu1
{
	width: 100%;
	height: 100px;
	margin-bottom: 5px;
	position: relative;
	z-index: 4;
}

.sky-lecciones .menu1:before
{
	content: ' ';
	z-index: 5;
	position: absolute;
	top: -18px;
	right: 50%;
	width: 50%;
	margin-right: -25%;
	height: 6px;
	border-top: solid 1px #FFF;
	border-bottom: dotted 1px #FFF;
}


.sky-lecciones .menu1 .nivel
{
	z-index: 5;
	position: absolute;
	top: 0px;
	left: 50%;
	width: 200px;
	margin-left: -100px;
	text-align: center;
	background-image: url('<%= asset_data_uri 'tpl/logo-nivel.png' %>');
	background-size: 100% auto;
	background-repeat: no-repeat;
	padding-top: 58px;
	font-size: 28px;
	color: #EECC33;
}

.sky-lecciones .menu1 a
{
	position: relative;
	color: #FFF;
}

.sky-lecciones .menu1 .anterior,
.sky-lecciones .menu1 .siguiente
{
	position: absolute;
	z-index: 5;
	font-size: 15px;
	text-align: center;
	padding-top: 37px;
}

.sky-lecciones .menu1 .anterior
{
	right: 50%;
	padding-top: 37px;
	padding-right: 120px;
	margin-right: 4%;
}

.sky-lecciones .menu1 .anterior a:after,
.sky-lecciones .menu1 .siguiente a:after
{
	content: ' ';
	position: absolute;
	top: -3px;
	width: 20px;
	height: 25px;
	background-repeat: no-repeat;
	background-size: 100% auto;
}

.sky-lecciones .menu1 .anterior a:after
{
	right: -30px;
	background-image: url('<%= asset_data_uri 'tpl/nivel-anterior.png' %>');
}

.sky-lecciones .menu1 .siguiente
{
	left: 50%;
	padding-top: 37px;
	padding-left: 120px;
	margin-left: 4%;
}

.sky-lecciones .menu1 .siguiente a:after
{
	left: -30px;
	background-image: url('<%= asset_data_uri 'tpl/nivel-siguiente.png' %>');
}

.sky-lecciones .leyenda
{
	position: relative;
	text-align: center;
	height: 40px;
	font-size: 15px;
	background-image: url('<%= asset_data_uri 'tpl/fondo-centro.png' %>');
	background-repeat: repeat-x;
	background-size: auto 100%;
	margin-left: 120px;
	margin-right: 120px;
	z-index: 4;
}

.sky-lecciones .leyenda:after,
.sky-lecciones .leyenda:before
{
	content: ' ';
	position: absolute;
	z-index: 5;
	top:0px;
	height: 40px;
	width: 20px;
	background-repeat: no-repeat;
	background-size: auto 100%;
}


.sky-lecciones .leyenda:after
{
	left: -20px;
	background-image: url('<%= asset_data_uri 'tpl/fondo-left.png' %>');
	background-position: right;
}

.sky-lecciones .leyenda:before
{
	right: -20px;
	background-image: url('<%= asset_data_uri 'tpl/fondo-right.png' %>');
	background-position: left;
}

.sky-lecciones .leyenda .center-xy
{
	width: 100%;
	height: 40px;
}

.sky-lecciones .leyenda .center-xy
{
	height: 40px;
}

.leccion-margen-contenido
{
	margin-left: 100px;
	margin-right: 100px;
}


.sky-lecciones .titulo
{
	margin-top: 15px;
}

.sky-lecciones .titulo .titulo-texto
{
	padding-left: 15px;
	padding-right: 15px;
	display: table-cell;
	height: 40px;
	vertical-align: middle;
	font-size: 25px;
	text-transform: uppercase;
	background-image: url('<%= asset_data_uri 'tpl/fondo-titulo-leccion.png' %>');
	background-repeat: repeat-x;
	background-size: auto 100%;
}

.sky-lecciones .contenido-wrapper
{
	position: relative;
	padding-left: 100px;
	padding-right: 100px;
	padding-top: 5px;
	padding-bottom: 30px;
}

.sky-lecciones .contenido-wrapper:after,
.sky-lecciones .contenido-wrapper:before
{
	position: absolute;
	top: 0px;
	content: ' ';
	width: 6px;
	height: 100%;
}


.sky-lecciones .contenido-wrapper:after
{
	left: 11px;
	border-left: solid 1px #FFF;
	border-right: dotted 1px #FFF;
}

.sky-lecciones .contenido-wrapper:before
{
	right: 11px;
	border-right: solid 1px #FFF;
	border-left: dotted 1px #FFF;
}


.sky-lecciones .contenido-wrapper .bloque1
{
	width: 100%;
	padding-right: 10px;
	position: relative;
	z-index: 4;
	color: #666;
}

.sky-lecciones .contenido-wrapper .bloque1 .imagen
{
	position: absolute;
	top: 0px;
	left: 0px;
	z-index: 4;
	border-top: solid 6px #EECC33;
	width: 45%;
	height: 100%;
	background-color: #FFF;
}

.sky-lecciones .contenido-wrapper .bloque1 .imagen:after,
.sky-lecciones .contenido-wrapper .bloque1 .textoleccion:after,
.sky-lecciones .contenido-wrapper .bloque1 .textoleccion:before
{
	z-index: 7;
	content: ' ';
	position: absolute;
	top: 0px;
	width: 4px;
	height: 100%;
	background-image: url('<%= asset_data_uri 'tpl/puntos-vertical-gris.png' %>');
	background-repeat: repeat-y;
	background-size: 100% auto;
}

.sky-lecciones .contenido-wrapper .bloque1 .imagen:after
{
	left: -2px;
}
.sky-lecciones .contenido-wrapper .bloque1 .textoleccion:after
{
	right: -2px;
}
.sky-lecciones .contenido-wrapper .bloque1 .textoleccion:before
{
	left: -2px;
}

.sky-lecciones .contenido-wrapper .bloque1 .textoleccion
{
	min-height: 400px;
	position: relative;
	border-top: solid 6px #EECC33;
	width: 35%;
	margin-left: 45%;
	background-color: #FFF;
	background-image: url('<%= asset_data_uri 'tpl/fondo-puntos-grises.png' %>');
	background-size: 30px auto;
	padding-top: 15px;
	padding-left: 15px;
	padding-right: 15px;
	padding-bottom: 30px;
	z-index: 4;
}

.sky-lecciones .contenido-wrapper .menu2
{
	border-top: solid 6px #EECC33;
	background-image: url('<%= asset_data_uri 'tpl/fondo-puntos-grises.png' %>');
	background-size: 30px auto;
	position: absolute;
	top: 0px;
	right: 0px;
	z-index: 4;
	width: 20%;
	height: 100%;
	background-color: #FFF;
}


.sky-lecciones .contenido-wrapper .bloque1 .textoleccion .titulonumerico
{
	font-size: 20px;
	text-transform: uppercase;
	text-decoration: underline;
	padding-bottom: 10px;
}

.sky-lecciones .contenido-wrapper .bloque1 .textoleccion .resumenleccion
{
	text-align: justify;
}

.sky-lecciones .contenido-wrapper .bloque1 .textoleccion .enlaceleccion
{
	bottom: 5px;
	right: 5px;
	position: absolute;
	z-index: 5;
	background-color: #EECC33;
}

.sky-lecciones .contenido-wrapper .bloque1 .textoleccion .enlaceleccion div
{
	display: table-cell;
	vertical-align: middle;
	text-transform: uppercase;
	height: 30px;
	padding-right: 10px;
	padding-left: 40px;
	color: #000;
	border-top: solid 1px #BBB;
	border-left: solid 1px #BBB;
	border-bottom: solid 1px #000;
	border-right: solid 1px #000;
	position: relative;
	z-index: 5;
}

.sky-lecciones .contenido-wrapper .bloque1 .textoleccion .enlaceleccion div:after
{
	position: absolute;
	z-index: 6;
	top: 0;
	left: 0px;
	width: 30px;
	height: 100%;
	border-right: solid 1px #BBB;
	text-align: center;
	padding-top: 4px;
	font-size: 17px;

	/* Estilos tomados de Glyphicon PLAY */
	content: '\e072'; 
	display:inline-block;
	font-family:'Glyphicons Halflings';
	font-style:normal;
	font-weight:normal;
	line-height:1;
	-webkit-font-smoothing:antialiased;
	-moz-osx-font-smoothing:grayscale
}

.sky-lecciones .contenido-wrapper .menu2
{
	padding-top: 10px;
	padding-left: 10px;
	padding-right: 10px;
	overflow: hidden;
}

.sky-lecciones .contenido-wrapper .menu2 div
{
	padding-left: 25px;
	padding-top: 3px;
	padding-bottom: 3px;
	position: relative;
	z-index: 4;
}

.sky-lecciones .contenido-wrapper .menu2 div:after
{
	position: absolute;
	z-index: 5;
	content: ' ';
	top: 0px;
	left: 0px;
	width: 40px;
	height: 100%;
	background-image: url('<%= asset_data_uri 'tpl/circulo-gris.png' %>');
	background-repeat: no-repeat;
	background-position: left center;
	background-size: 15px 15px;
}


.sky-lecciones .contenido-wrapper .menu2 div.actual:after
{
	background-image: url('<%= asset_data_uri 'tpl/circulo-amarillo.png' %>');
}

.sky-lecciones .contenido-wrapper .menu2 a
{
	color: #000;
	font-size: 12px;
}

.sky-lecciones .contenido-wrapper .menu2 .actual a
{
	font-weight: bold;
}

/* Inicio middle header */
