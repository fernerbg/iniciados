.sky-novedades-body
{
	padding-top: 55px;
	padding-bottom: 50px;
	background-color: #005580;
	background-image: url('<%= asset_data_uri 'tpl/fondo-novedades.jpg' %>');
	background-size: 100% auto;
	background-repeat: repeat-y;
	position: relative;
	z-index: 4;
}

.sky-novedades-body:after
{
	content: ' ';
	position: absolute;
	z-index: 5;
	width: 90%;
	height: 6px;
	top: 40px;
	left: 5%;
	border-top: solid 1px #FFF;
	border-bottom: solid 2px #FFF;
}

.sky-novedades-body .mes .nombre-mes
{
	width: 100%;
	text-align: center;
	font-size: 30px;
	color: #FFF;
	text-transform: uppercase;
}

.sky-novedades-body .mes .entradas
{
	min-height: 300px;
	position: relative;
	padding-top: 120px;
	padding-left: 80px;
	padding-right: 80px;
}

.sky-novedades-body .mes .entradas:after
{
	content: ' ';
	position: absolute;
	top: 0px;
	left: 50%;
	width: 0px;
	height: 100%;
	border: 1px solid #FFF;
}

.sky-novedades-body .mes .entradas .entrada
{
	margin-top: -100px;
	width: 50%;
	min-height: 140px;
	color: #000;
}

.sky-novedades-body .mes .entradas .entrada .fecha
{
	position: relative;
	z-index: 4;
	text-transform: uppercase;
	border-bottom: 6px solid #EECC33;
	color: #FFF;
}
.sky-novedades-body .mes .entradas .entrada .contenido
{
	background-color: #FFF;
	padding-top: 20px;
	padding-bottom: 30px;
	padding-left: 30px;
	padding-right: 30px;
}

.sky-novedades-body .mes .entradas .entrada .contenido .titulonovedad
{
	font-size: 25px;
	padding-bottom: 10px;
}

/* personalizacoines PAR - IMPAR */
.sky-novedades-body .mes .entradas .entrada:nth-child(odd)
{
}

.sky-novedades-body .mes .entradas .entrada:nth-child(even)
{
	margin-left: 50%;
}
.sky-novedades-body .mes .entradas .entrada:nth-child(odd) .fecha
{
	padding-right: 50px;
	text-align: right;
}
.sky-novedades-body .mes .entradas .entrada:nth-child(even) .fecha
{
	padding-left: 50px;
	text-align: left;
}

.sky-novedades-body .mes .entradas .entrada .fecha:after
{
	position: absolute;
	content:' ';
	top: 8px;
	width: 30px;
	height: 30px;
	background-image: url('<%= asset_data_uri 'tpl/esfera_blanca.png' %>');
	background-repeat: no-repeat;
	background-size: 100% 100%;
}
.sky-novedades-body .mes .entradas .entrada:nth-child(odd) .fecha:after
{
	right: -14px;
}
.sky-novedades-body .mes .entradas .entrada:nth-child(even) .fecha:after
{
	left: -14px;
}

.sky-novedades-body .mes .entradas .entrada:nth-child(odd) .contenido
{
	margin-right: 50px;
}

.sky-novedades-body .mes .entradas .entrada:nth-child(even) .contenido
{
	margin-left: 50px;
}


/* COLORES DE LAS ESFERAS HOME Y OTROS... */
.pagina-lecciones .sky-header .link-evd,
.pagina-lecciones .sky-middle-header .novedades-titulo
{
	background-color: #EECC33;
}

.pagina-home .sky-header .link-evd,
.pagina-home .sky-middle-header .novedades-titulo
{
	background-color: #E6622C;
	color: #FFF;
}
